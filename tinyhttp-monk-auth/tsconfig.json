{
	"compilerOptions": {
		"target": "es2019",
		"module": "commonjs",
		"declaration": true,
		"outDir": "dist",
		"strict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"esModuleInterop": true
	},
	"exclude": [
		"node_modules",
		"dist"
	]
}